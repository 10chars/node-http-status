{
  "name": "node-http-status",
  "version": "1.1.0",
  "description": "Simple and intuitive HTTP response status codes",
  "author": "luke.helg@gmail.com",
  "keywords": [
    "http",
    "status",
    "response",
    "codes",
    "simple"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/10chars/node-http-status"
  },
  "bugs": {
    "url": "https://github.com/10chars/node-http-status/issues"
  },
  "homepage": "https://github.com/10chars/node-http-status#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./locale/en": {
      "import": {
        "types": "./dist/locales/en.d.ts",
        "default": "./dist/locales/en.js"
      },
      "require": {
        "types": "./dist/locales/en.d.cts",
        "default": "./dist/locales/en.cjs"
      }
    },
    "./locale/ja": {
      "import": {
        "types": "./dist/locales/ja.d.ts",
        "default": "./dist/locales/ja.js"
      },
      "require": {
        "types": "./dist/locales/ja.d.cts",
        "default": "./dist/locales/ja.cjs"
      }
    },
    "./locale/es": {
      "import": {
        "types": "./dist/locales/es.d.ts",
        "default": "./dist/locales/es.js"
      },
      "require": {
        "types": "./dist/locales/es.d.cts",
        "default": "./dist/locales/es.cjs"
      }
    },
    "./locale/de": {
      "import": {
        "types": "./dist/locales/de.d.ts",
        "default": "./dist/locales/de.js"
      },
      "require": {
        "types": "./dist/locales/de.d.cts",
        "default": "./dist/locales/de.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --fix",
    "test": "npm run build && node --test test/index.test.js",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.40.0"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module"
}
